{
  "name": "tourze/bundle-dependency",
  "description": "Bundle dependency resolver for Symfony",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tourze",
      "email": "tourze@example.com"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\BundleDependency\\": "src/"
    }
