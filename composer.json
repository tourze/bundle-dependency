{
  "name": "tourze/bundle-dependency",
  "description": "Bundle dependency resolver for Symfony",
  "type": "library",
  "require": {
    "php": "^8.1",
    "symfony/http-kernel": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\BundleDependency\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\BundleDependency\\Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Tourze",
      "email": "tourze@example.com"
    }
  ]
}